{
  "name": "timi-monorepo",
  "version": "1.0.0",
  "description": "Timi monorepo with Convex migration",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "deploy": "turbo run deploy",
    "migration": "cd packages/migration && npm run migrate"
  },
  "devDependencies": {
    "turbo": "^1.10.0"
  }
}
